+  Архивирование и сжатие файлов
_ "разархивировать bzip2 файл" "bunzip2 file1.bz2"
_ "разархивировать gzip файл" "gunzip file1.gz"
_ "сжать файл gzip" "gzip file1"
_ "сжать файл bzip2" "bzip2 file1"
_ "максимальное сжатие gzip" "gzip -9 file1"
_ "создать rar-архив (1 файл)" "rar a file1.rar test_file"
_ "создать rar-архив (несколько файлов)" "rar a file1.rar file1 file2 dir1"
_ "распаковать rar-архив" "unrar x file1.rar"
_ "создать tar-архив (1 файл)" "tar -cvf archive.tar file1"
_ "создать tar-архив (несколько файлов)" "tar -cvf archive.tar file1 file2 dir1"
_ "просмотреть содержимое tar-архива" "tar -tf archive.tar"
_ "распаковать tar-архив" "tar -xvf archive.tar"
_ "распаковать tar в указанную директорию" "tar -xvf archive.tar -C /tmp"
_ "создать bzip2 сжатый tar-архив" "tar -cvfj archive.tar.bz2 dir1"
_ "распаковать bzip2 сжатый tar-архив" "tar -xvfj archive.tar.bz2"
_ "создать gzip сжатый tar-архив" "tar -cvfz archive.tar.gz dir1"
_ "распаковать gzip сжатый tar-архив" "tar -xvfz archive.tar.gz"
_ "создать zip-архив" "zip file1.zip file1"
_ "создать zip-архив (рекурсивно)" "zip -r file1.zip file1 file2 dir1"
_ "распаковать zip-архив" "unzip file1.zip"
